{
  "expo": {
    "name": "Refs",
    "slug": "refsnyc",
    "version": "0.0.39",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "scheme": "refsnyc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "jsEngine": "jsc",
    "deepLinks": ["refsnyc://reset-password", "refsnyc://invite/g"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "refsnyc.app",
      "buildNumber": "140",
      "associatedDomains": ["applinks:refs.nyc"],
      "config": {
        "usesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "production"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Refs to use your camera so you can capture photos for your refs.",
        "NSPhotoLibraryUsageDescription": "Allow Refs to access your photos so you can share them as refs.",
        "NSPhotoLibraryAddUsageDescription": "Allow Refs to save shared photos to your library when you download them.",
        "NSUserNotificationUsageDescription": "Allow Refs to send notifications so you don\u2019t miss new messages or community activity."
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F3F2ED"
        }
      ],
      "expo-share-intent",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Refs to use your location to show people nearby."
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "da57a930-b9ea-4cf9-b722-b2ff8bbf7208"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "refs-nyc",
    "android": {
      "package": "refsnyc.app",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    }
  }
}
